<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main2ActivityInicio">

    <TextView
            android:text="Inicio"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/textViewInicio" android:textSize="36sp"
            android:layout_marginTop="53dp" android:layout_marginBottom="20dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textViewNombreInicio" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:layout_width="253dp"
            android:layout_height="0dp"
            android:id="@+id/textViewNombreInicio"
            app:layout_constraintTop_toBottomOf="@+id/textViewInicio" android:layout_marginBottom="19dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textViewEmailInicio"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:layout_width="253dp"
            android:layout_height="0dp"
            android:id="@+id/textViewEmailInicio"
            app:layout_constraintTop_toBottomOf="@+id/textViewNombreInicio" android:layout_marginBottom="47dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textViewLongInicio"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:id="@+id/textViewLatInicio"
            app:layout_constraintBaseline_toBaselineOf="@+id/textViewLongInicio"
            app:layout_constraintEnd_toStartOf="@+id/textViewLongInicio" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="79dp" android:layout_marginStart="79dp" android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textViewLongInicio"
            app:layout_constraintTop_toBottomOf="@+id/textViewEmailInicio"
            app:layout_constraintStart_toEndOf="@+id/textViewLatInicio" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="57dp" app:layout_constraintBottom_toTopOf="@+id/buttonGetDosInicio"
            android:layout_marginEnd="79dp" android:layout_marginRight="79dp"/>
    <Button
            android:text="GET"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:id="@+id/buttonGetUnoInicio"
            app:layout_constraintBaseline_toBaselineOf="@+id/buttonGetDosInicio"
            app:layout_constraintEnd_toStartOf="@+id/buttonGetDosInicio" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="79dp" android:layout_marginStart="79dp" android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"/>
    <Button
            android:text="GET"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/buttonGetDosInicio"
            app:layout_constraintTop_toBottomOf="@+id/textViewLongInicio"
            app:layout_constraintStart_toEndOf="@+id/buttonGetUnoInicio" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="23dp" app:layout_constraintBottom_toTopOf="@+id/editTextGetUnoInicio"
            android:layout_marginEnd="79dp" android:layout_marginRight="79dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="51dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextGetDosInicio"
            app:layout_constraintBaseline_toBaselineOf="@+id/editTextGetUnoInicio"
            app:layout_constraintStart_toEndOf="@+id/editTextGetUnoInicio" android:layout_marginEnd="79dp"
            android:layout_marginRight="79dp" app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextGetUnoInicio" app:layout_constraintTop_toBottomOf="@+id/buttonGetDosInicio"
            app:layout_constraintEnd_toStartOf="@+id/editTextGetDosInicio" android:layout_marginBottom="50dp"
            app:layout_constraintBottom_toTopOf="@+id/buttonGuardarInicio" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="76dp" android:layout_marginStart="76dp" android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"/>
    <Button
            android:text="Guardar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonGuardarInicio"
            app:layout_constraintTop_toBottomOf="@+id/editTextGetUnoInicio" android:layout_marginBottom="88dp"
            app:layout_constraintEnd_toEndOf="@+id/editTextGetDosInicio"
            app:layout_constraintStart_toStartOf="@+id/editTextGetUnoInicio" android:layout_marginLeft="84dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="84dp"
            android:layout_marginEnd="84dp" android:layout_marginRight="84dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>